<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MauiApp5.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MauiApp5.Pages"
    Shell.FlyoutBehavior="Flyout"
    Shell.TabBarIsVisible="False">



    <FlyoutItem
        FlyoutDisplayOptions="AsMultipleItems">
        <ShellContent
            Title="Minecraft"
            ContentTemplate="{DataTemplate local:Minecraft}"
            Icon="minecraft.png"/>
        <ShellContent
            Title="Tekken"
            ContentTemplate="{DataTemplate local:Tekken}" 
            Icon="tekken.png"/>
        <ShellContent
            Title="Genshin Impact"
            ContentTemplate="{DataTemplate local:Genshin}" 
            Icon="genshin.png"/>
    </FlyoutItem>

</Shell>
